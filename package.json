{
  "name": "qylon",
  "version": "1.0.0",
  "description": "AI Automation Platform - Transform manual business processes into intelligent, self-running systems",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:api-gateway\" \"npm run dev:meeting-intelligence\" \"npm run dev:workflow-automation\"",
    "dev:api-gateway": "cd services/api-gateway && npm run dev",
    "dev:meeting-intelligence": "cd services/meeting-intelligence && python -m uvicorn main:app --reload --port 3003",
    "dev:workflow-automation": "cd services/workflow-automation && npm run dev",
    "build": "npm run build:api-gateway && npm run build:meeting-intelligence && npm run build:workflow-automation",
    "build:api-gateway": "cd services/api-gateway && npm run build",
    "build:meeting-intelligence": "cd services/meeting-intelligence && pip install -r requirements.txt",
    "build:workflow-automation": "cd services/workflow-automation && npm run build",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "cypress run",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "format": "prettier --write .",
    "setup": "node scripts/setup/init.js",
    "deploy:staging": "node scripts/deployment/deploy-staging.js",
    "deploy:production": "node scripts/deployment/deploy-production.js",
    "infrastructure:plan": "cd infrastructure/terraform && terraform plan",
    "infrastructure:apply": "cd infrastructure/terraform && terraform apply",
    "infrastructure:destroy": "cd infrastructure/terraform && terraform destroy",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "ai",
    "automation",
    "meeting-intelligence",
    "workflow",
    "microservices",
    "supabase",
    "digitalocean"
  ],
  "author": "KD Squares Development Team",
  "license": "PROPRIETARY",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "concurrently": "^8.2.1",
    "cypress": "^13.3.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.10.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "redis": "^4.6.8",
    "axios": "^1.5.0",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KD-Squares/KDS-Development.git"
  },
  "bugs": {
    "url": "https://github.com/KD-Squares/KDS-Development/issues"
  },
  "homepage": "https://github.com/KD-Squares/KDS-Development#readme"
}